<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.linkbasic.animatortest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".animation.FrameActivity" />
        <activity android:name=".animation.TranslateActivity" />
        <activity android:name=".animation.ScaleActivity" />
        <activity android:name=".animation.RotateActivity" />
        <activity android:name=".animation.AlphaActivity" />
        <activity android:name=".animation.AnimationSetActivity" />
        <activity android:name=".animator.ValueAnimatorActivity" />
        <activity android:name=".animator.ValueAnimator2Activity" />
        <activity android:name=".animator.ObjectAnimatorActivity" />
        <activity android:name=".animator.ObjectAnimator2Activity"></activity>
    </application>

</manifest>